    "            if str(cand) not in sys.path:\n",
    "                sys.path.insert(0, str(cand))\n",
    "            return cand\n",
    "    return None\n",
    "_ensure_repo_on_path()\n",
    "from mcs_mea_analysis.pairings import PairingIndex"
